cat >/var/www/html/vless-$user.txt <<-END
-----------------------------------------
UnderTunnel PROJECT
-----------------------------------------
Format Vless gRPC (SNI)
-----------------------------------------
- name: Vless-$user-gRPC (SNI)
server: ${domain}
port: 443
type: vless
uuid: ${uuid}
cipher: auto
tls: true
skip-cert-verify: true
servername: ${domain}
network: grpc
grpc-opts:
grpc-mode: gun
grpc-service-name: vless-grpc
udp: true
-----------------------------------------
Link Vless Account
-----------------------------------------
Link GRPC : vless://${uuid}@${domain}:443?mode=gun&security=tls&encryption=none&type=grpc&serviceName=vless-grpc&sni=${domain}#${user}
-----------------------------------------
END
vlesslink3="vless://${uuid}@${domain}:443?mode=gun&security=tls&encryption=none&type=grpc&serviceName=vless-grpc&sni=${domain}#${user}"
systemctl restart xray
systemctl restart nginx
service cron restart
if [ ! -e /etc/vless ]; then
mkdir -p /etc/vless
fi
if [ -z ${Quota} ]; then
Quota="0"
fi
c=$(echo "${Quota}" | sed 's/[^0-9]*//g')
d=$((${c} * 1024 * 1024 * 1024))
if [[ ${c} != "0" ]]; then
echo "${d} ${iplim}" >/etc/vless/${user}
fi
DATADB=$(cat /etc/vless/.vless.db | grep "^###" | grep -w "${user}" | awk '{print $2}')
if [[ "${DATADB}" != '' ]]; then
sed -i "/${user}/d" /etc/vless/.vless.db
fi
echo "### ${user} ${exp}" >>/etc/vless/.vless.db
clear
echo -e "${g}┌────────────────────────┐${p}" | tee -a /etc/vless/${user}.log
echo -e "${b}    Xray/Vless Account     ${p}" | tee -a /etc/vless/${user}.log
echo -e "${g}└────────────────────────┘${p}" | tee -a /etc/vless/${user}.log
echo -e "Remarks      : ${user}" | tee -a /etc/vless/${user}.log
echo -e "Domain       : ${domain}" | tee -a /etc/vless/${user}.log
echo -e "Location     : $CITY" | tee -a /etc/vless/${user}.log
echo -e "User Quota   : ${Quota} GB" | tee -a /etc/vless/${user}.log
echo -e "Limit Adress : ${iplim} IP" | tee -a /etc/vless/${user}.log
echo -e "Port TLS     : 443" | tee -a /etc/vless/${user}.log
echo -e "Port NTLS    : 80" | tee -a /etc/vless/${user}.log
echo -e "Port GRPC    : 443" | tee -a /etc/vless/${user}.log
echo -e "User ID      : ${uuid}" | tee -a /etc/vless/${user}.log
echo -e "AlterId      : 0" | tee -a /etc/vless/${user}.log
echo -e "Security     : auto" | tee -a /etc/vless/${user}.log
echo -e "Network      : WS or gRPC" | tee -a /etc/vless/${user}.log
echo -e "Path         : (/vless) " | tee -a /etc/vless/${user}.log
echo -e "Path Dynamic : CF-XRAY:https://bug.com " | tee -a /etc/vless/${user}.log
echo -e "ServiceName  : vless-grpc" | tee -a /etc/vless/${user}.log
echo -e "${g}───────────────────────────${p}" | tee -a /etc/vless/${user}.log
echo -e "Link GRPC    : ${vlesslink3}" | tee -a /etc/vless/${user}.log
echo -e "${g}───────────────────────────${p}" | tee -a /etc/vless/${user}.log
echo -e "Link Account : https://${domain}:81/vless-$user.txt" | tee -a /etc/vless/${user}.log
echo -e "${g}───────────────────────────${p}" | tee -a /etc/vless/${user}.log
echo -e "Expired On : $exp" | tee -a /etc/vless/${user}.log
echo -e "${g}───────────────────────────${p}" | tee -a /etc/vless/${user}.log
echo -e "" | tee -a /etc/vless/${user}.log
read -n 1 -s -r -p "Press [ Enter ] to back on menu"
vless
)
clear
grenbo="${gb}"
NC='${p}'
echo -e "${g}┌──────────────────────────────────────────┐${p}"
echo -e "${g}│${b}        ADD ACCOUNT VLESS              ${p}"
echo -e "${g}└──────────────────────────────────────────┘${p}"
echo -e "${g}┌──────────────────────────────────────────┐${p}"
echo -e "${g}│  ${gb}[1]•${b} Creating Vless ( All )${p}"
echo -e "${g}│  ${gb}[2]•${b} Creating Vless WS${p}"
echo -e "${g}│  ${gb}[3]•${b} Creating Vless gRPC${p}"
echo -e "${g}└──────────────────────────────────────────┘${p}"
echo -e ""
read -p "Select From Options [ 1 - 4 ] : " menu
case $menu in
1)
add_all
;;
2)
add_ws
;;
3)
add_grpc
;;
*)
echo -e "${Red}You wrong command !${Font}"
sleep 1
vless
;;
esac
